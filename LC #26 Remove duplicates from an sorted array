class Solution {
public:
    int removeDuplicates(vector<int>& nums) {

        if(nums.empty())
        {
            return 0;
        }
        int n = nums.size();

        int p1 = 0;
        //int p2 = 0;

        for(int p2 = 1 ; p2 < n ;p2++)
        {
            if(nums[p2] != nums[p1])
            {
                p1++;
                nums[p1] = nums[p2];

            }
            
        }
        
        return p1 + 1;
    }
};
